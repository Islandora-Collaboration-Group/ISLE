<?xml version="1.0"?>
<definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="3">
  <properties>
    <root_name>mads</root_name>
    <schema_uri>http://www.loc.gov/standards/mads/mads.xsd</schema_uri>
    <namespaces default="http://www.loc.gov/mads/v2">
      <namespace prefix="xlink">http://www.w3.org/1999/xlink</namespace>
      <namespace prefix="xsi">http://www.w3.org/2001/XMLSchema-instance</namespace>
      <namespace prefix="mads">http://www.loc.gov/mads/v2</namespace>
    </namespaces>
  </properties>
  <form>
    <properties>
      <type>form</type>
      <access>TRUE</access>
      <attributes>
        <schemaLocation/>
        <enctype>multipart/form-data</enctype>
      </attributes>
      <required>FALSE</required>
      <tree>TRUE</tree>
    </properties>
    <children>
      <element name="authority">
        <properties>
          <type>markup</type>
          <access>TRUE</access>
          <collapsible>FALSE</collapsible>
          <required>FALSE</required>
          <title>Authorized Form of Name</title>
          <tree>TRUE</tree>
          <actions>
            <create>
              <path>/mads:mads</path>
              <context>document</context>
              <schema/>
              <type>element</type>
              <prefix>NULL</prefix>
              <value>authority</value>
            </create>
            <read>
              <path>/mads:mads/mads:authority</path>
              <context>document</context>
            </read>
            <update>
              <path>self::node()</path>
              <context>self</context>
            </update>
            <delete>NULL</delete>
          </actions>
        </properties>
        <children>
          <element name="name">
            <properties>
              <type>fieldset</type>
              <access>TRUE</access>
              <required>FALSE</required>
              <tree>TRUE</tree>
              <actions>
                <create>
                  <path>/mads:mads/mads:authority</path>
                  <context>document</context>
                  <schema/>
                  <type>xml</type>
                  <prefix>NULL</prefix>
                  <value>&lt;name type="corporate"&gt;%value%&lt;/name&gt;</value>
                </create>
                <read>
                  <path>/mads:mads/mads:authority/mads:name[@type='corporate']</path>
                  <context>document</context>
                </read>
                <update>
                  <path>self::node()</path>
                  <context>self</context>
                </update>
                <delete>
                  <path>self::node()</path>
                  <context>self</context>
                </delete>
              </actions>
            </properties>
            <children>
              <element name="auth_dept">
                <properties>
                  <type>textfield</type>
                  <access>TRUE</access>
                  <required>FALSE</required>
                  <title>Department Name</title>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>/mads:mads/mads:authority/mads:name</path>
                      <context>document</context>
                      <schema/>
                      <type>element</type>
                      <prefix>NULL</prefix>
                      <value>namePart</value>
                    </create>
                    <read>
                      <path>/mads:mads/mads:authority/mads:name[@type='corporate']/mads:namePart[not(@*)]</path>
                      <context>document</context>
                    </read>
                    <update>
                      <path>self::node()</path>
                      <context>self</context>
                    </update>
                    <delete>
                      <path>self::node()</path>
                      <context>self</context>
                    </delete>
                  </actions>
                </properties>
                <children/>
              </element>
              <element name="auth_date">
                <properties>
                  <type>textfield</type>
                  <access>TRUE</access>
                  <attributes>
                    <type>date</type>
                  </attributes>
                  <collapsed>FALSE</collapsed>
                  <collapsible>FALSE</collapsible>
                  <description>The start date that the department name became official in the format YYYY</description>
                  <disabled>FALSE</disabled>
                  <executes_submit_callback>FALSE</executes_submit_callback>
                  <multiple>FALSE</multiple>
                  <required>FALSE</required>
                  <resizable>FALSE</resizable>
                  <title>Date</title>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>/mads:mads/mads:authority/mads:name</path>
                      <context>document</context>
                      <schema/>
                      <type>xml</type>
                      <prefix>NULL</prefix>
                      <value>&lt;namePart type='date'&gt;%value%&lt;/namePart&gt;</value>
                    </create>
                    <read>
                      <path>/mads:mads/mads:authority/mads:name[@type='corporate']/mads:namePart[@type='date']</path>
                      <context>document</context>
                    </read>
                    <update>
                      <path>self::node()</path>
                      <context>self</context>
                    </update>
                    <delete>
                      <path>self::node()</path>
                      <context>self</context>
                    </delete>
                  </actions>
                </properties>
                <children/>
              </element>
            </children>
          </element>
        </children>
      </element>
      <element name="related_Parent">
        <properties>
          <type>markup</type>
          <access>TRUE</access>
          <collapsible>FALSE</collapsible>
          <required>FALSE</required>
          <title>Parent School/Faculty</title>
          <tree>TRUE</tree>
          <actions>
            <create>
              <path>/mads:mads</path>
              <context>document</context>
              <schema/>
              <type>xml</type>
              <prefix>NULL</prefix>
              <value>&lt;related type="parentOrg"&gt;%value%&lt;/related&gt;</value>
            </create>
            <read>
              <path>/mads:mads/mads:related[@type='parentOrg']</path>
              <context>document</context>
            </read>
            <update>
              <path>self::node()</path>
              <context>self</context>
            </update>
            <delete>
              <path>self::node()</path>
              <context>self</context>
            </delete>
          </actions>
        </properties>
        <children>
          <element name="related_Parentname">
            <properties>
              <type>markup</type>
              <access>TRUE</access>
              <required>FALSE</required>
              <tree>TRUE</tree>
              <actions>
                <create>
                  <path>self::node()</path>
                  <context>parent</context>
                  <schema/>
                  <type>xml</type>
                  <prefix>NULL</prefix>
                  <value>&lt;name type="corporate"&gt;%value%&lt;/name&gt;</value>
                </create>
                <read>
                  <path>mads:name[@type='corporate']</path>
                  <context>parent</context>
                </read>
                <update>
                  <path>self::node()</path>
                  <context>self</context>
                </update>
                <delete>
                  <path>self::node()</path>
                  <context>self</context>
                </delete>
              </actions>
            </properties>
            <children>
              <element name="related_ParentnamePart">
                <properties>
                  <type>textfield</type>
                  <access>TRUE</access>
                  <required>FALSE</required>
                  <title>Parent School/Faculty</title>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>self::node()</path>
                      <context>parent</context>
                      <schema/>
                      <type>element</type>
                      <prefix>NULL</prefix>
                      <value>namePart</value>
                    </create>
                    <read>
                      <path>mads:namePart[not(@*)]</path>
                      <context>parent</context>
                    </read>
                    <update>
                      <path>self::node()</path>
                      <context>self</context>
                    </update>
                    <delete>
                      <path>self::node()</path>
                      <context>self</context>
                    </delete>
                  </actions>
                </properties>
                <children/>
              </element>
            </children>
          </element>
        </children>
      </element>
      <element name="relatedPage">
        <properties>
          <type>tabs</type>
          <access>TRUE</access>
          <required>FALSE</required>
          <title>Previous Name(s)</title>
          <tree>TRUE</tree>
        </properties>
        <children>
          <element name="related">
            <properties>
              <type>tabpanel</type>
              <access>TRUE</access>
              <collapsible>FALSE</collapsible>
              <required>FALSE</required>
              <title>Previous Names of the Dept/Org</title>
              <tree>TRUE</tree>
              <actions>
                <create>
                  <path>/mads:mads</path>
                  <context>document</context>
                  <schema/>
                  <type>xml</type>
                  <prefix>NULL</prefix>
                  <value>&lt;related type="earlier"&gt;%value%&lt;/related&gt;</value>
                </create>
                <read>
                  <path>/mads:mads/mads:related[@type='earlier']</path>
                  <context>document</context>
                </read>
                <update>
                  <path>self::node()</path>
                  <context>self</context>
                </update>
                <delete>
                  <path>self::node()</path>
                  <context>self</context>
                </delete>
              </actions>
            </properties>
            <children>
              <element name="related_name">
                <properties>
                  <type>fieldset</type>
                  <access>TRUE</access>
                  <required>FALSE</required>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>self::node()</path>
                      <context>parent</context>
                      <schema/>
                      <type>xml</type>
                      <prefix>NULL</prefix>
                      <value>&lt;name type="corporate"&gt;%value%&lt;/name&gt;</value>
                    </create>
                    <read>
                      <path>mads:name[@type='corporate']</path>
                      <context>parent</context>
                    </read>
                    <update>
                      <path>self::node()</path>
                      <context>self</context>
                    </update>
                    <delete>
                      <path>self::node()</path>
                      <context>self</context>
                    </delete>
                  </actions>
                </properties>
                <children>
                  <element name="related_namePart">
                    <properties>
                      <type>textfield</type>
                      <access>TRUE</access>
                      <required>FALSE</required>
                      <title>Previous Name(s)</title>
                      <tree>TRUE</tree>
                      <actions>
                        <create>
                          <path>self::node()</path>
                          <context>parent</context>
                          <schema/>
                          <type>element</type>
                          <prefix>NULL</prefix>
                          <value>namePart</value>
                        </create>
                        <read>
                          <path>mads:namePart[not(@*)]</path>
                          <context>parent</context>
                        </read>
                        <update>
                          <path>self::node()</path>
                          <context>self</context>
                        </update>
                        <delete>
                          <path>self::node()</path>
                          <context>self</context>
                        </delete>
                      </actions>
                    </properties>
                    <children/>
                  </element>
                  <element name="related_date">
                    <properties>
                      <type>textfield</type>
                      <access>TRUE</access>
                      <attributes>
                        <type>date</type>
                      </attributes>
                      <collapsed>FALSE</collapsed>
                      <collapsible>FALSE</collapsible>
                      <description>The start date and end date of the previous departmental name in the format YYYY-YYYY</description>
                      <disabled>FALSE</disabled>
                      <executes_submit_callback>FALSE</executes_submit_callback>
                      <multiple>FALSE</multiple>
                      <required>FALSE</required>
                      <resizable>FALSE</resizable>
                      <title>Date</title>
                      <tree>TRUE</tree>
                      <actions>
                        <create>
                          <path>self::node()</path>
                          <context>parent</context>
                          <schema/>
                          <type>xml</type>
                          <prefix>NULL</prefix>
                          <value>&lt;namePart type='date'&gt;%value%&lt;/namePart&gt;</value>
                        </create>
                        <read>
                          <path>mads:namePart[@type='date']</path>
                          <context>parent</context>
                        </read>
                        <update>
                          <path>self::node()</path>
                          <context>self</context>
                        </update>
                        <delete>
                          <path>self::node()</path>
                          <context>self</context>
                        </delete>
                      </actions>
                    </properties>
                    <children/>
                  </element>
                </children>
              </element>
            </children>
          </element>
        </children>
      </element>
      <element name="identifier_u2">
        <properties>
          <type>textfield</type>
          <access>TRUE</access>
          <required>FALSE</required>
          <title>U2 Identifier</title>
          <tree>TRUE</tree>
          <actions>
            <create>
              <path>/mads:mads</path>
              <context>document</context>
              <schema/>
              <type>xml</type>
              <prefix>NULL</prefix>
              <value>&lt;identifier type="u2"&gt;%value%&lt;/identifier&gt;</value>
            </create>
            <read>
              <path>/mads:mads/mads:identifier[@type='u2']</path>
              <context>document</context>
            </read>
            <update>
              <path>self::node()</path>
              <context>self</context>
            </update>
            <delete>
              <path>self::node()</path>
              <context>self</context>
            </delete>
          </actions>
        </properties>
        <children/>
      </element>
      <element name="note">
        <properties>
          <type>textarea</type>
          <access>TRUE</access>
          <collapsible>FALSE</collapsible>
          <required>FALSE</required>
          <title>Historical Note</title>
          <tree>TRUE</tree>
          <actions>
            <create>
              <path>/mads:mads</path>
              <context>document</context>
              <schema/>
              <type>xml</type>
              <prefix>NULL</prefix>
              <value>&lt;note type="historical"&gt;%value%&lt;/note&gt;</value>
            </create>
            <read>
              <path>/mads:mads/mads:note[@type='historical']</path>
              <context>document</context>
            </read>
            <update>
              <path>self::node()</path>
              <context>self</context>
            </update>
            <delete>NULL</delete>
          </actions>
        </properties>
        <children/>
      </element>
      <element name="url">
        <properties>
          <type>textfield</type>
          <access>TRUE</access>
          <collapsible>FALSE</collapsible>
          <description>Link to the faculty/school/departmental page.</description>
          <required>FALSE</required>
          <title>URL</title>
          <tree>TRUE</tree>
          <actions>
            <create>
              <path>/mads:mads</path>
              <context>document</context>
              <schema/>
              <type>element</type>
              <prefix>NULL</prefix>
              <value>url</value>
            </create>
            <read>
              <path>/mads:mads/mads:url</path>
              <context>document</context>
            </read>
            <update>
              <path>self::node()</path>
              <context>self</context>
            </update>
            <delete>NULL</delete>
          </actions>
        </properties>
        <children/>
      </element>
    </children>
  </form>
</definition>
